pipeline 
{
  agent any
  stages
  {
       stage('Build') 
       {
        steps 
          {
            echo 'Building..'
            sh 'clean'
          }
        }
       stage('Build dockerimage')
        {
         steps
         {
          echo "Building dockerimage"
          script
          {
           dockerImage = docker.buildregistry +":$BUILD_NUMBER"
          }
         }
        }
   }
}
